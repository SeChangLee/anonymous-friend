model Member {
  memberId              Int         @id @default(autoincrement()) 
  email                 String?     @unique
  password              String?     
  nickname              String?     
  mbti                  String?     
  birthday              String?     
  gender                String?     
  stateId               Int?        
  emailCertKey          String?     
  emailCertDate         DateTime    
  withdrawalApplyDate   DateTime    
  createDate            DateTime?   @default(now())
}