generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test {
  memberId            Int       @id @default(autoincrement())
  email               String?   @unique
  password            String?
  nickname            String?
  mbti                String?
  birthday            String?
  gender              String?
  stateId             Int?
  emailCertKey        String?
  emailCertDate       DateTime
  withdrawalApplyDate DateTime
  createDate          DateTime? @default(now())
}

model member_state {
  state_id    Int      @id(map: "member_state_pk") @unique(map: "member_state_idx_01") @default(autoincrement())
  name        String   @db.VarChar(15)
  create_date DateTime @default(dbgenerated("CURRENT_TIMESTAMP(3)")) @db.Timestamp(6)
}

model member_stateeee {
  state_id    Int      @id @default(autoincrement())
  name        String   @db.VarChar(15)
  create_date DateTime @default(dbgenerated("CURRENT_TIMESTAMP(3)")) @db.Timestamp(6)
}
